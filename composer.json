{
    "name":        "swarrot/mail-worker",
    "description": "A worker to send mails",
    "keywords": ["swarrot","worker","mail"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Olivier Dolbeau",
            "homepage": "http://odolbeau.fr/"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "ext-amqp": "*",
        "swarrot/swarrot": "dev-master",
        "swarrot/ack-processor": "~1.0",
        "symfony/console": "~2.4",
        "swiftmailer/swiftmailer": "~4.1"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": {
            "Swarrot": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
